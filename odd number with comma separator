Input = 5

Output = 1,3,5

program

#include <stdio.h>

int main() 
{
  int n,i,flag;
  scanf("%d",&n);
  i=1;
  flag=0;
  while(i<=n)
  {
      if(i%2!=0)
      {
          if(flag==0)
          {
              printf("%d",i);
              flag=1;
          } 
          else
          {
              printf(",%d",i);
          }
      }
      i++;
  }
}
